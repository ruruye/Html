<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
				"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
				
<mapper namespace="prod">
	
	<select id="lguProd" resultType="prodVo" parameterType="String">
		select prod_id, prod_name from prod where prod_lgu = #{data}
	</select>
	
	
	<select id="idProd" resultType="prodVo" parameterType="String">
		select * from prod where prod_id = #{data}
	</select>
</mapper>